{
    "scriptFile": "__init__.py",
    "bindings": [
      {
        "name": "inputblob",
        "type": "blobTrigger",
        "direction": "in",
        "path": "input-container/{name}.json",
        "connection": "AzureWebJobsStorage"
      },
      {
        "name": "outputblob",
        "type": "blob",
        "direction": "out",
        "path": "output-container/result-{name}",
        "connection": "AzureWebJobsStorage"
      }
    ]
  }
  