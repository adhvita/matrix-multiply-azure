{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  },
  "extensions": {
    "queues": {
      "batchSize": 1,
      "maxDequeueCount": 5,
      "newBatchThreshold": 0
    },
    "blobs": {
      "maxDegreeOfParallelism": 1
    }
  }
}
